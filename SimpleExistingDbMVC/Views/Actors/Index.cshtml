@model IEnumerable<SimpleExistingDbMVC.Models.Actors>

@{ 
    ViewBag.Title = "Actors";
}

<h2>Actors</h2>

<p>
    <a asp-controller="Actors" asp-action="Create">Create New</a>
</p>

<table class="table">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Age</th>
        <th>AcademyWinner</th>
    </tr>
    
    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Age)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AcademyWinner)
        </td>
    </tr>
    }
</table>